<!DOCTYPE html>
<html lang="en">
	<head>
		<title>Welcome</title>

		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
		<meta name="description" content="Page on BerliozCuistock">
		<meta name="author" content="">

		{{ entrypoints('app', 'css', {preload: true}) }}
	</head>
	<body class="container">
		<nav class="navbar navbar-expand-lg navbar-dark fixed-top px-md-5 bg-dark">
			<div class="container-fluid">
				<a class="navbar-brand" href="#">Cuistock</a>
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse" id="navbarSupportedContent">
					<ul class="navbar-nav me-auto mb-2 mb-lg-0">
						<li class="nav-item">
							<a class="nav-link" aria-current="page" href="{{ path('home') }}">Accueil</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="{{ path('recipes') }}">Recipes</a>
						</li>

					</ul>
					<div class="navbar-left">
						<a href="{{ path('user') }}" class="bucket-svg">Register</a>
					</div>
				</div>
			</div>
		</nav>
		{% block body %}{% endblock %}
		{{ entrypoints('app', 'js', {preload: true}) }}
		<link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600,700" rel="stylesheet">
		{{ include('@Berlioz-HttpCore/Twig/Debug.twig') }}
	</body>
</html>
